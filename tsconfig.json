{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "types": ["react", "react-dom", "vite/client"],
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": "."
  },
  "include": ["src", "artist.json", "taller.json"]
}
